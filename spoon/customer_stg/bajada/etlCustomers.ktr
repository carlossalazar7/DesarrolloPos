    <sql>SELECT DISTINCT&#xd;&#xa;        &#x24;&#x7b;POS_STORE&#x7d; AS &#x60;store&#x60;,&#xd;&#xa;        &#x60;ve&#x60;.&#x60;terminal&#x60; AS &#x60;cash_machine&#x60;,&#xd;&#xa;        DATE_FORMAT&#x28;&#x60;ve&#x60;.&#x60;fechanegocio&#x60;, &#x27;&#x25;d&#x2f;&#x25;m&#x2f;&#x25;Y&#x27;&#x29; AS &#x60;business_date&#x60;,&#xd;&#xa;        &#x60;ve&#x60;.&#x60;id&#x60; AS &#x60;tran_seq_no&#x60;,&#xd;&#xa;        &#x60;c&#x60;.&#x60;nombre&#x60; AS &#x60;customer_name&#x60;,&#xd;&#xa;        &#x60;c&#x60;.&#x60;email&#x60; AS &#x60;customer_email&#x60;,&#xd;&#xa;        &#x60;c&#x60;.&#x60;telefono&#x60; AS &#x60;customer_phone&#x60;,&#xd;&#xa;        &#x60;cdd&#x60;.&#x60;direccion&#x60; AS &#x60;customer_address&#x60;,&#xd;&#xa;        &#x60;tdc&#x60;.&#x60;nombre&#x60; AS &#x60;customer_document&#x60;,&#xd;&#xa;        &#x60;c&#x60;.&#x60;sexo&#x60; AS &#x60;customer_genre&#x60;,&#xd;&#xa;        &#x60;tcm&#x60;.&#x60;nombre&#x60; AS &#x60;customer_type&#x60;&#xd;&#xa;    FROM&#xd;&#xa;        &#x28;&#x28;&#x28;&#x28;&#x28;&#x28;&#x28;&#x60;silverpos&#x60;.&#x60;clientes&#x60; &#x60;c&#x60;&#xd;&#xa;        JOIN &#x60;silverpos&#x60;.&#x60;tipo_doc_cliente&#x60; &#x60;tdc&#x60; ON &#x28;&#x28;&#x60;c&#x60;.&#x60;id_tipo_doc&#x60; &#x3d; &#x60;tdc&#x60;.&#x60;id&#x60;&#x29;&#x29;&#x29;&#xd;&#xa;        JOIN &#x60;silverpos&#x60;.&#x60;tipo_cliente_maestro&#x60; &#x60;tcm&#x60; ON &#x28;&#x28;&#x60;c&#x60;.&#x60;tipo_cliente&#x60; &#x3d; &#x60;tcm&#x60;.&#x60;id&#x60;&#x29;&#x29;&#x29;&#xd;&#xa;        JOIN &#x60;silverpos&#x60;.&#x60;clientes_direcciones_domicilios&#x60; &#x60;cdd&#x60; ON &#x28;&#x28;&#x60;c&#x60;.&#x60;id&#x60; &#x3d; &#x60;cdd&#x60;.&#x60;id_cliente&#x60;&#x29;&#x29;&#x29;&#xd;&#xa;        JOIN &#x60;fiscal_el_salvador&#x60;.&#x60;caja_documento&#x60; &#x60;cd&#x60;&#x29;&#xd;&#xa;        JOIN &#x60;silverpos&#x60;.&#x60;variables_configuracion&#x60; &#x60;vc&#x60;&#x29;&#xd;&#xa;        JOIN &#x60;ventasdiarias&#x60;.&#x60;fecha_negocio&#x60; &#x60;fn&#x60;&#x29;&#xd;&#xa;        JOIN &#x60;ventasdiarias&#x60;.&#x60;venta_encabezado&#x60; &#x60;ve&#x60;&#x29;&#xd;&#xa;    WHERE&#xd;&#xa;        &#x28;&#x60;ve&#x60;.&#x60;idcliente&#x60; &#x3d; &#x60;c&#x60;.&#x60;id&#x60;&#x29;</sql>
