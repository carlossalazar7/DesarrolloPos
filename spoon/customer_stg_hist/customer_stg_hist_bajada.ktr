    <sql>SELECT DISTINCT&#xa;        &#x24;&#x7b;POS_STORE&#x7d; AS &#x27;store&#x27;,&#xa;        hve.terminal AS &#x27;cash_machine&#x27;,&#xa;        DATE_FORMAT&#x28;hve.fechanegocio, &#x27;&#x25;d&#x2f;&#x25;m&#x2f;&#x25;Y&#x27;&#x29; AS &#x27;business_date&#x27;,&#xa;        hve.id AS &#x27;tran_seq_no&#x27;,&#xa;        hc.nombre AS &#x27;customer_name&#x27;,&#xa;         c.email  AS &#x27;customer_email&#x27;,&#xa;         c.telefono  AS &#x27;customer_phone&#x27;,&#xa;        cdd.direccion  AS &#x27;customer_address&#x27;,&#xa;        tdc.nombre  AS &#x27;customer_document&#x27;,&#xa;        c.sexo  AS &#x27;customer_gender&#x27;,&#xa;        tcm.nombre  AS &#x27;customer_type&#x27;&#xa;    FROM&#xa;			silverpos_hist.hist_clientes hc, silverpos_hist.hist_venta_enca hve, &#xa;			silverpos.clientes c , silverpos.clientes_direcciones_domicilios cdd,&#xa;			silverpos.tipo_doc_cliente tdc, silverpos.tipo_cliente_maestro tcm&#xa;        	where hve.idcliente &#x3d; hc.id&#xa;        	and hc.nombre&#x3d;c.nombre&#xa;        	and cdd.id_cliente &#x3d; c.id&#xa;        	and tdc.id &#x3d; c.id_tipo_doc&#xa;        	and tcm.id &#x3d; c.tipo_cliente&#xa;        	and hc.nombre &#x21;&#x3d; &#x27;NA&#x27;&#xa;			and hve.fechanegocio BETWEEN &#x24;&#x7b;POS_DATE_START&#x7d; and &#x24;&#x7b;POS_DATE_END&#x7d;&#x3b;</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
